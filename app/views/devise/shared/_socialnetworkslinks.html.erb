<%- if devise_mapping.omniauthable? %>
    <ul class="list-inline text-center hidden-xs">
      <%- resource_class.omniauth_providers.each do |provider| %>
      <li><%= link_to "Sign in with #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider), class:"btn btn-primary list-inline"%>
      </li>
      <% end -%>
    </ul>
<% end -%>
